{
  "name": "kaf305",
  "version": "0.0.1",
  "description": "Single-page application for the 305 department of the Moscow Aviation Institute. It is built on the basis of the Quasar Framework. The architecture is based on the composition-api.",
  "productName": "KAF-305",
  "author": "NZ-WEB <stereo00@yandex.ru>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue ./ --fix",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "generate:icons": "icongenie g -p ./icongenie-prod.json",
    "test:unit": "jest --updateSnapshot",
    "test:unit:ci": "jest --ci",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "dev": "quasar dev",
    "build": "quasar build"
  },
  "dependencies": {
    "@quasar/extras": "^1.13.3",
    "animate.css": "^4.1.1",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "dotenv": "^16.0.1",
    "quasar": "^2.6.1",
    "simple-keyboard": "^3.4.57",
    "simple-keyboard-layouts": "^3.0.202",
    "tailwindcss": "^2.2.7",
    "vue": "^3.0.0",
    "vue-i18n": "^9.0.0-beta.0",
    "vue-router": "^4.0.0",
    "vuex": "^4.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.14",
    "@quasar/app": "^3.3.3",
    "@quasar/quasar-app-extension-dotenv": "^1.1.0",
    "@quasar/quasar-app-extension-qenv": "^1.1.0",
    "@quasar/quasar-app-extension-testing": "^2.0.2",
    "@quasar/quasar-app-extension-testing-unit-jest": "^3.0.0-alpha.9",
    "electron": "^13.1.9",
    "electron-packager": "^15.2.0",
    "eslint": "^8.10.0",
    "eslint-plugin-vue": "^8.5.0",
    "eslint-config-prettier": "^8.1.0",
    "prettier": "^2.5.1",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-webpack-plugin": "^2.4.0"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 12.22.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
